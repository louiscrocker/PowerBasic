    TimeCheck      AS DWORD       ' in US
    DemoteLimit    AS DWORD       ' in US
    PromoteLimit   AS DWORD       ' in US

    ' Percentage based information
    DemotePercent  AS BYTE
    PromotePercent AS BYTE
    Spare(0 TO 1)  AS BYTE

    ' Flags
    Flags          AS DWORD
'    DWORD AllowDemotion:1;       ' first bit
'    DWORD AllowPromotion:1;      ' second bit
'    DWORD Reserved:30;           ' everything else

END TYPE

' processor power policy
TYPE PROCESSOR_POWER_POLICY
    Revision        AS DWORD      ' 1

    ' Dynamic Throttling Policy
    DynamicThrottle AS BYTE
    Spare(0 TO 2)   AS BYTE

    ' Flags
    Flags           AS DWORD
'    DWORD DisableCStates:1;      ' first bit
'    DWORD Reserved:31;           ' all the rest

    ' System policy information
    ' The Array is last, in case it needs to be grown and the structure
    ' revision incremented.
    PolicyCount     AS DWORD
    Policy(0 TO 2)  AS PROCESSOR_POWER_POLICY_INFO

END TYPE

