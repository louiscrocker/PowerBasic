' *****************
' FACILITY_SETUPAPI
' *****************
'
' Since these error codes aren't in the standard Win32 range (i.e., 0-64K), define a
' macro to map either Win32 or SetupAPI error codes into an HRESULT.
'
#IF %DEF(%USEMACROS)
    MACRO HRESULT_FROM_SETUPAPI (x) = IIF&(((x) AND (%APPLICATION_ERROR_MASK OR %ERROR_SEVERITY_ERROR)) = (%APPLICATION_ERROR_MASK OR %ERROR_SEVERITY_ERROR), ((x) AND &H0000FFFF) OR (%FACILITY_SETUPAPI * &H10000) OR &H80000000???), HRESULT_FROM_WIN32(x))
#ELSE
    FUNCTION HRESULT_FROM_SETUPAPI (BYVAL x AS LONG) AS LONG
        IF (((x) AND (%APPLICATION_ERROR_MASK OR %ERROR_SEVERITY_ERROR)) = (%APPLICATION_ERROR_MASK OR %ERROR_SEVERITY_ERROR)) THEN
            FUNCTION = (((x) AND &H0000FFFF) OR (%FACILITY_SETUPAPI * &H10000) OR &H80000000???)
        ELSE
            FUNCTION = HRESULT_FROM_WIN32(x)
        END IF
    END FUNCTION
#ENDIF

